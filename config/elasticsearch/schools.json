{
  "settings": {
    "number_of_shards": 1,
    "analysis": {
      "filter": {
        "autocomplete_filter": {
          "type": "edge_ngram",
          "min_gram": 1,
          "max_gram": 20
        }
      },
      "analyzer": {
        "autocomplete": {
          "type": "custom",
          "tokenizer": "standard",
          "filter": [
            "lowercase",
            "autocomplete_filter"
          ]
        }
      }
    }
  },
  "mappings": {
    "school": {
      "_all": {
        "enabled": false
      },
      "properties": {
        "urn": {
          "type": "keyword"
        },
        "name": {
          "type": "string",
          "analyzer": "autocomplete"
        },
        "la_name": {
          "type": "keyword"
        }
      }
    }
  }
}
